<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F4ABAB"
    tools:context=".fragments.HordeFragment">


    <TextView
        android:id="@+id/DHHordePVP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="12dp"
        android:text="@string/pvp"
        android:textAlignment="center"
        android:textColor="#9040FF"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.752"
        app:layout_constraintStart_toEndOf="@+id/DHHordeRaids"
        app:layout_constraintTop_toBottomOf="@+id/DHHorde" />

    <TextView
        android:id="@+id/DHHordePVP2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="  0   1   2   3"
        android:textAlignment="center"
        android:textColor="#9040FF"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.634"
        app:layout_constraintStart_toEndOf="@+id/DHHordeDungeons3"
        app:layout_constraintTop_toBottomOf="@+id/DHHordePVP" />

    <SeekBar
        android:id="@+id/seekBar3"
        android:layout_width="117dp"
        android:layout_height="23dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toTopOf="@+id/DHHordePVP2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.812"
        app:layout_constraintStart_toEndOf="@+id/seekBar2"
        app:layout_constraintTop_toBottomOf="@+id/DHHordePVP"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/DHHordeRaids"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/raids"
        android:textAlignment="center"
        android:textColor="#9040FF"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.261"
        app:layout_constraintStart_toEndOf="@+id/DHHordeDungeons"
        app:layout_constraintTop_toBottomOf="@+id/DHHorde" />

    <SeekBar
        android:id="@+id/seekBar2"
        android:layout_width="117dp"
        android:layout_height="23dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="5dp"
        app:layout_constraintStart_toEndOf="@+id/seekBar"
        app:layout_constraintTop_toBottomOf="@+id/DHHordeRaids" />

    <TextView
        android:id="@+id/DHHordeDungeons3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="4dp"
        android:text="  0   2   5   8"
        android:textAlignment="center"
        android:textColor="#9040FF"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/DHHordeDungeons2"
        app:layout_constraintTop_toBottomOf="@+id/seekBar2" />

    <TextView
        android:id="@+id/DHHorde"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/dh"
        android:textAlignment="center"
        android:textColor="#9040FF"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="1dp" />

    <TextView
        android:id="@+id/DHHordeDungeons"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/dungeons"
        android:textAlignment="center"
        android:textColor="#9040FF"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/DHHorde" />

    <TextView
        android:id="@+id/DHHordeDungeons2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="  0   1   4   8"
        android:textAlignment="center"
        android:textColor="#9040FF"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@+id/DHHordeDungeons"
        tools:layout_editor_absoluteX="0dp" />

    <SeekBar
        android:id="@+id/seekBar"
        android:layout_width="117dp"
        android:layout_height="23dp"
        app:layout_constraintBottom_toTopOf="@+id/DHHordeDungeons2"
        tools:layout_editor_absoluteX="0dp" />
</androidx.constraintlayout.widget.ConstraintLayout>